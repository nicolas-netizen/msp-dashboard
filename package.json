{
  "name": "msp-dashboard",
  "version": "1.0.0",
  "description": "Dashboard para MSP Manager con reportes de tickets y horas",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["msp", "dashboard", "tickets", "reportes"],
  "author": "MSP Team",
  "license": "MIT"
}

